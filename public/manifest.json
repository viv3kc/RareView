{
  "name": "Rare View",
  "description": "RareView - ",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [ "<all_urls>" ],
  "permissions": [
    "scripting",
    "activeTab",
    "webRequest",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/bundle.js"],
      "css": ["build/bundle.css"],
      "run_at": "document_end"
    }
  ]
}